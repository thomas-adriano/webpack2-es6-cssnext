{
    "name": "webpack2-es6-cssnext",
    "version": "0.0.1",
    "description": "Webpack 2 + ES6 (Babel) + CSSNext (PostCSS)",
    "main": "src/index.js",
    "author": "thomas.o.adriano@gmail.com",
    "license": "MIT",
    "browser": [
        "last 3 versions"
    ],
    "scripts": {
        "build": "rm -rdf ./build && mkdir build && webpack --colors --progress --display-error-details",
        "serve": "yarn run build && http-server ./build",
        "live": "webpack-dev-server --colors --progress --hot --inline --host 0.0.0.0"
    },
    "dependencies": {
        "font-awesome": "^4.7.0",
        "normalize.css": "^5.0.0"
    },
    "devDependencies": {
        "css-loader": "^0.26.1",
        "file-loader": "^0.9.0",
        "html-loader": "^0.4.4",
        "html-webpack-plugin": "^2.26.0",
        "image-webpack-loader": "^3.1.0",
        "style-loader": "^0.13.1",
        "url-loader": "^0.5.7",
        "webpack": "^2.2.0",
        "webpack-dev-server": "^2.2.0"
    }
}
