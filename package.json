{
    "name": "webpack2-es6-cssnext",
    "version": "0.0.1",
    "description": "Webpack 2 + ES6 (Babel) + CSSNext (PostCSS)",
    "main": "src/index.js",
    "author": "thomas.o.adriano@gmail.com",
    "license": "MIT",
    "browser": [
        "last 3 versions"
    ],
    "scripts": {
        "build": "rm -rdf ./build && mkdir ./build && webpack --colors --progress --display-error-details --config webpack/webpack-prod.config.js",
