        "live": "webpack-dev-server --colors --progress --hot --inline --host 0.0.0.0 --config webpack/webpack-dev.config.js"
    },
    "dependencies": {},
    "devDependencies": {
        "autoprefixer": "^6.6.1",
        "babel-core": "^6.21.0",
        "babel-loader": "^6.2.10",
        "babel-preset-es2015": "^6.18.0",
        "css-loader": "^0.26.1",
        "extract-loader": "^0.1.0",
        "extract-text-webpack-plugin": "^2.0.0-beta",
        "file-loader": "^0.9.0",
        "html-loader": "^0.4.4",
        "html-webpack-plugin": "^2.26.0",
        "normalize.css": "^5.0.0",
        "postcss-cssnext": "^2.9.0",
        "postcss-loader": "^1.2.2",
        "postcss-modules": "^0.6.2",
        "posthtml-css-modules": "^0.1.1",
        "posthtml-load-config": "^1.0.0",
        "posthtml-loader": "^0.11.0",
        "style-loader": "^0.13.1",
        "url-loader": "^0.5.7",
        "webpack": ">=2.2.0-rc.3 <3.0.0",
        "webpack-dev-server": ">=2.2.0-rc.0 <3.0.0"
    }
}